    "@emotion/styled": "^11.11.0",
    "@tsconfig/node18": "^1.0.1",
    "dotenv": "^16.0.1",
    "framer-motion": "^10.12.17",
    "npm-run-all": "^4.1.5",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "reflect-metadata": "^0.1.13",
